@model I_RAY_ASSET_TRACKER_MVC.Models.UpdateAssetModel

<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.Label("Team : ")
                </div>
            </div>
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.Team, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="row">
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.Label("HW/SW Name : ")
                </div>
            </div>
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.HWSWName, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="row">
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.Label("Serial Number : ")
                </div>
            </div>
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.SerialNumberVersionNumber, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="row">
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.Label("Asset Type : ")
                </div>
            </div>
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.AssetType, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="row">
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.Label("InvoiceNumber : ")
                </div>
            </div>
            <div class="form-group col-md-6">
                <div class="col-md-12">
                    @Html.TextBoxFor(m => m.InvoiceNo, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </div>
</div>